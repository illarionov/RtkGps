<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android" >
    
    <EditTextPreference
        android:key="stream_file_filename"
        android:title="@string/file_filename_title"
        android:summary="@string/file_filename_summary"
        android:defaultValue=""
        />
    <CheckBoxPreference
		android:key="syncdropbox"
		android:title="@string/file_syncdropbox_title"
		android:summary="@string/file_syncdropbox_summary"
        />
    <CheckBoxPreference
		android:key="zipbeforesync"
		android:title="@string/file_zipbeforesync_title"
		android:summary="@string/file_zipbeforesync_summary"
		android:dependency="syncdropbox"
        />

</PreferenceScreen>